<script id="ProjectTemplateForm" type="text/x-jquery-tmpl">
    <tr>
    <td>
        <label class="lblint">Name</label>
    </td>
    <td>
        <input type="textbox" name="name" id="name" value="${name}" />
			
    </td>    
    <td>
        <label class="lblint">Description</label>
    </td>
    <td>    
        <input type="text" name="description" id="description" value="${description}" />          
    </td>
    
    
   
</tr>

<tr>    
    <td>
        <label class="lblint">Projection</label>
    </td>
    <td>    
        {{if (projection)}}
         <input type="textbox" name="projection.code" id="projection" value="${projection.code}" />    
         {{else}}
            <input type="textbox" name="projection.code" id="project_Projection" value="" />        
        {{/if}}
    </td>
    
    <td>
        <label class="lblint">Display Projection</label>
    </td>
    <td>
        {{if (displayProjection)}}  
		<input type="textbox" name="displayProjection.code" id="displayProjection" value="${displayProjection.code}" /> 
        {{else}}
            <input type="textbox" name="displayProjection.code" id="displayProjection" value="" />
        {{/if}}          
    </td>
    
</tr>
<input type="hidden" name="maxResolutions" id="maxResolutions" value="0" />
 <input type="hidden" name="minResolutions" id="minResolutions" value="0" />


<tr>
    <td>
       <label class="lblint">Max Extent</label>
    </td>
    <td>
        <input type="textbox" name="maxextent" id="maxextent" value="${maxextent}" />
    </td>
    <td>
        <label class="lblint">Min Extent</label>
    </td>
    <td>
        <input type="textbox" name="minextent" id="minextent" value="${minextent}" />
    </td>
    
</tr>
<tr>
    <td>
        <label class="lblint">NumZoom Levels</label>
    </td>
    <td>
        <input type="textbox" name="numzoomlevels" id="numzoomlevels" value="${numzoomlevels}" />        

    </td>
    <td>
        <label class="lblint">Restricted Extent</label>
    </td>
    <td>
        <input type="textbox" name="restrictedextent" id="restrictedextent" value="${restrictedextent}" />
    </td>
    
</tr>
<tr>
    <td>
        <label class="lblint">Output Format</label>
    </td>
    <td>
        <select name="outputFormat.name" id="project_outputFormat">
        <option  value="">Please Select</option>         
        </select>
    </td>
    <td>
        <label class="lblint">Unit</label>
    </td>

    <td>
        <select name="unit.name" id="project_unit">
            <option  value="">Please Select</option>			
			        
        </select>
    </td>    
</tr>
<input type="hidden" name="copyright" id="copyright" value="" />
<input type="hidden" name="watermask" id="watermask" value="" />
<!--tr>
    <td>
        CopyRight
    </td>
    <td>
        <input type="textbox" name="copyright" id="copyright" value="${copyright}" />
    </td>
    
    <td>
        Water Mask
    </td>
    <td>
        <input type="textbox" name="watermask" id="watermask" value="${watermask}" />
    </td>
     
    
</tr-->



<tr>
     <td>&nbsp;</td>
     <td>
      <input type="hidden" id="active" name="active" value="${active}" />
       {{if (active)}}       
       		<input type="checkbox" id="active1" name="active1" value="${active}"  CHECKED onClick="changeActiveValue(this);" />   Active        
       {{else}}
       		<input type="checkbox" id="active1" name="active1" value="${active}" onClick="changeActiveValue(this);"/>   Active 
       {{/if}}
       
       
		
    </td>
	<td>&nbsp;</td>
    <td>
      <input type="hidden" id="cosmetic" name="cosmetic" value="${cosmetic}"/>
      
	   {{if (cosmetic)}}
       
       <input type="checkbox" id="cosmetic1" name="cosmetic1" value="${cosmetic}"  CHECKED onClick="changeCosmeticValue(this);" />   Cosmetic 
       
       {{else}}

       <input type="checkbox" id="cosmetic1" name="cosmetic1" value="${cosmetic}" onClick="changeCosmeticValue(this);"/>   Cosmetic 
       
       
						
                    
     {{/if}}
       
       
		
    </td>
     
</tr>

<tr>
     <td>
        <label class="lblint">Active Layer</label>
    </td>
     <td >
      <select name="activelayer" id = "activelayer">
       <option SELECTED value="">Please Select</option>
    </td>
    
    <td>
        <label class="lblint">Overview Layer</label>
    </td>
     <td>
     <select name="overlaymap" id = "overlaymap">
       <option SELECTED value="">Please Select</option>
    </td>
    
</tr>

</script>



    
<script id="ProjectTemplateDisclaimer" type="text/x-jquery-tmpl">
<tr>
<td colspan="4">
<p><input type="checkbox" id="chkDisclaimer" name="chkDisclaimer"  onchange="toggleDisclaimer(this);"  />   Disclaimer </p>

<textarea spellcheck="false" name="disclaimer" id="Disclaimer" value="${disclaimer}" cols=118 rows=2 style="visibility: hidden; height:70px;width:700px;"></textarea>
</td>
</tr>

</script>



