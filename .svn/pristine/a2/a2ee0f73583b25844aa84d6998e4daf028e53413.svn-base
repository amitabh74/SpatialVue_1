	top: 50%;
	margin-top: -8px;
	left: 25%;
	margin-left: -8px;
}

a.fg-button {
	float: left;
}

/* remove extra button width in IE 
	button.fg-button { width:36px; overflow:visible; }
*/
.fg-button-icon-left {
	padding-left: 2.1em;
}

.fg-button-icon-right {
	padding-right: 2.1em;
}

.fg-button-icon-left .ui-icon {
	right: auto;
	left: .2em;
	margin-left: 0;
}

