/* Saurabh */

ul.menuBar,ul.menuBar ul {
	display: block;
	margin: 0px;
	padding: 0px;
	background-color: #c00;
}

ul.menuBar li {
	display: inline;
	list-style: none;
	position: relative;
	margin: 0px;
	padding: 15px 15px 15px 15px;
}

ul.menuBar li ul {
	display: block;
	position: absolute;
	left: 0;
	top: 1em;
	visibility: hidden;
	width: 10em;
	z-index: 1000;
	background-color: #fff;
	border: 1px solid #000;
	border-top: 0;
}

ul.menuBar a {
	color: #ffffff;
}

ul.menuBar li ul li {
	margin: 0 0 0 -1.5em;
	padding: 0;
	display: block;
	width: 100%;
	margin: 0px 0px 5px 0px;
	background-color: #fff;
	color: #000;
}

ul.menuBar li ul li a {
	display: block;
	margin: 0;
	padding: 0 0 0 5%;
	width: 100%;
	width: 105% %;
	color: #000;
}

ul.menuBar li.submenu:hover {
	padding-bottom: 30em;
}

ul.menuBar li.submenu:hover ul {
	left: 0;
	visibility: visible;
}

ul.menuBar li.submenu:hover ul li {
	color: #c00;
	margin: 5;
}

ul.menuBar li.submenu:hover ul li:hover {
	background-color: #c00;
}

/* End Saurabh */

